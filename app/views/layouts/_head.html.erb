<head>
  <%= stylesheet_link_tag 'compiled/screen.css', :media => 'screen, projection' %>
  <%= stylesheet_link_tag 'compiled/print.css', :media => 'print' %>
  <!--[if lt IE 9]>
  <%= stylesheet_link_tag 'compiled/ie', :media => 'screen, projection' %>
  <![endif]-->
  <script src="<%= javascript_path "load" %>" type="text/javascript" charset="utf-8"></script>
  <script type="text/javascript" charset="utf-8">
    head.js(
      "https://ajax.googleapis.com/ajax/libs/jquery/1.5.1/jquery.min.js",
      "<%= javascript_path "rails" %>",
      "<%= javascript_path "application" %>",
      "<%= javascript_path "jquery.sap" %>",
      "<%= javascript_path "jquery.rest_in_place" %>"
    );
  </script>
  <%= csrf_meta_tag %>
  <%= favicon_link_tag %>
  <% if @title %>
  <title>RoadDSS Value Calculator: <%= @title %></title>
  <% else %>
  <title>Vaisala RoadDSS Value Calculator</title>
  <% end %>
</head>
